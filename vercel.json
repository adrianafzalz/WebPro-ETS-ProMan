{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/public/index.php"
        },
        {
        "src": "/CSS/(.*)",
        "dest": "/public/CSS/$1"
        },
        {
        "src": "/images/(.*)",
        "dest": "/public/images/$1"
        },
        {
        "src": "/(favicon.ico|robots.txt|css|js|images|fonts|storage/.*)",
        "dest": "/public/$1"
        },
        { 
            "src": "/(CSS|images|css|js|images|vendor|assets|build)/(.+)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://web-pro-ets-pro-man.vercel.app",
        "APP_KEY": "add API key here from your .env file",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}